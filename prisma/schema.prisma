datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Course {
  id                 Int    @id @default(autoincrement())
  course_id          String
  course_number      String
  course_subject     String
  title              String
  description        String
  enrollment_info    String
  credit_min         Int
  credit_max         Int
  prerequisites      String[]
  // Additional fields added as needed
}